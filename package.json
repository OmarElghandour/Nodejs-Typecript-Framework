{
  "name": "nodejs-express-typescript-docker",
  "version": "1.0.0",
  "description": "Node.js Express application with TypeScript running inside Docker Compose",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "start:dev": "nodemon --watch ./src --ext ts --exec ts-node ./src/index.ts",
    "generate": "npx plop"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "inquirer": "^9.2.10",
    "mongoose": "^7.4.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1"
  }
}
